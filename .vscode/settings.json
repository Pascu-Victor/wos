    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.errorSquiggles": "enabled",
    "terminal.integrated.profiles.linux": {
        "WOS Development": {
            "path": "bash",
            "args": [
                "-c",
                "source ~/.bashrc 2>/dev/null || true; source '${workspaceFolder}/.vscode/wos-env.sh'; exec bash"
            ],
            "icon": "tools"
        },
        "WOS Development (Clean)": {
            "path": "bash",
            "args": [
                "--init-file",
                "${workspaceFolder}/.vscode/wos-env.sh"
            ],
            "icon": "gear"
        }
    },
    "terminal.integrated.defaultProfile.linux": "WOS Development",
    "C_Cpp.default.compilerPath": "${workspaceFolder}/toolchain/target1/bin/clang",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/toolchain/target1/include",
        "${workspaceFolder}/toolchain/target1/include/c++/v1",
        "${workspaceFolder}/toolchain/target1/lib/clang/21/include",
        "${workspaceFolder}/modules/defines",
        "${workspaceFolder}/modules/extern"
    ],
    "C_Cpp.default.defines": [
        "WOS=1",
        "__x86_64__=1"
    ],
    "C_Cpp.default.compilerArgs": [
        "--target=x86_64-pc-wos",
        "--sysroot=${workspaceFolder}/toolchain/target1",
    ],
}